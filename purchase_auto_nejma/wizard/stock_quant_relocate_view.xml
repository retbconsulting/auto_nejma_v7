        <field name="model">stock.quant.relocate</field>
        <field name="inherit_id" ref="stock.stock_quant_relocate_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='dest_location_id']" position="attributes">
                <attribute name="string">Destination</attribute>
